<div class="">
<%= form_with url: session_path do |form| %>
  
  <p>
    <%= form.label :email %><br>
    <%= form.email_field :email, class: "" %>
  </p>
  <p>
      <%= form.label :name %><br>
      <%= form.text_field :name, class: "" %>
  </p>
  <p>
    <%= form.label :password %><br>
    <%= form.password_field :password, class: "" %>
  </p>
  <p>
    <%= form.submit class: "" %>
  </p>
 
<% end %>
</div>